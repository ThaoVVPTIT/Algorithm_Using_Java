package cdac.node;

public class SortedSigleList implements SortedList{
	private SLNode head;
	public SortedSigleList()
	{
		this.head = null;
	}
	@Override
	public void insert(int element) {
		SLNode newNode = new SLNode(element);
		if(head == null)
		{
			head = newNode;
		}
		else
		{
			SLNode current = head;
			while(current != null)
			{
				if(current.data > newNode.data)
				{
					
				}
			}
		}
	}

