package cdac.acts.remove;

public class RemoveElementByValue {
	public static int removeElementByValue(int arr[], int val)
	{
		int k = 0;
		boolean checking = false;
		for(int i = 0; i < arr.length; i++)
		{
			if(arr[i] != val)
			{
				arr[k] = arr[i];
				k++;
			}
		}
		return k;
	}
	public static void main(String arg[])
	{
		System.out.println("Arrays after remove element by Value");
		
	}
}
